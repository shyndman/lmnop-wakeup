# Home Assistant API token
# Documentation: https://developers.home-assistant.io/docs/auth_api/
HASS_API_TOKEN=

# API key for the Gemini model
# Documentation: https://ai.google.dev/gemini-api/docs/api-key
GEMINI_API_KEY=

# API key for the Pirate Weather service
# Documentation: https://docs.pirateweather.net/en/latest/API/
PIRATE_WEATHER_API_KEY=

# API key for the Google Routes API
# Documentation: https://developers.google.com/maps/documentation/routes/get-api-key
GOOGLE_ROUTES_API_KEY=

# OpenTelemetry endpoint for exporting traces and logs (used by logfire)
# Documentation: https://logfire.pydantic.dev/docs/how-to-guides/alternative-backends/
OTEL_EXPORTER_OTLP_ENDPOINT=

# Headers for the OpenTelemetry exporter (used by logfire)
# Documentation: https://logfire.pydantic.dev/docs/how-to-guides/alternative-backends/
OTEL_EXPORTER_OTLP_HEADERS=

# Langfuse configuration
# Documentation: https://langfuse.com/docs
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=

# PostgreSQL database connection string
POSTGRES_CONNECTION_STRING=

# LiteLLM API URL
LITELLM_API_URL=

# LiteLLM API key
LITELLM_API_KEY=

# Music Assistant server URL
MUSIC_ASSISTANT_URL=http://music.don:8095

# Music Assistant player ID for announcements
MUSIC_ASSISTANT_PLAYER_ID=bedroom_speaker

# Base URL that Music Assistant can use to reach back to this server
WAKEUP_SERVER_BASE_URL=http://192.168.86.208:8002
